{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@slices": [
        "src/redux/slices"
      ],
      "@slices/*": [
        "src/redux/slices/*"
      ],
      "@store": [
        "src/redux/store"
      ],
      "@store/*": [
        "src/redux/*"
      ],
      "@resources/*": [
        "src/resources/*"
      ],
      "@schemas/*": [
        "src/schemas/*"
      ],
      "@schemas": [
        "src/schemas"
      ],
      "@assets": [
        "src/assets"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@config": [
        "src/config"
      ],
      "@components": [
        "src/components"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@routes": [
        "src/routes"
      ],
      "@routes/*": [
        "src/routes/*"
      ]
    }
  },
  "include": ["src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
